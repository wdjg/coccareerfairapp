{
  "name": "careerfairapp",
  "version": "1.0.0",
  "description": "Coc Career Fair App",
  "main": "./dist/app.js",
  "scripts": {
    "start": "npm run build; node ./dist/app.js",
    "dev": "npm run watch; nodemon --watch dist",
    "build": "babel src -d dist --ignore src/tests --copy-files",
    "watch": "babel -w src -d dist --ignore src/tests --copy-files",
    "clean": "rm -rf dist",
    "test": "NODE_ENV=test mocha --exit --compilers js:babel-register --recursive ./src/tests/"
  },
  "author": "bwang330@gatech.edu",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "glob": "^7.1.2",
    "js-beautify": "^1.7.5",
    "mocha": "^5.0.0",
    "nodemon": "^1.14.11",
    "sinon": "^4.2.2"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cacheman": "^2.2.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "jsonwebtoken": "^8.1.0",
    "logger": "0.0.1",
    "mongoose": "^4.13.9",
    "mongoose-id-validator": "^0.4.3",
    "mongoose-validatorjs": "^0.7.1",
    "morgan": "^1.9.0",
    "node-cron": "^1.2.1",
    "nodemailer": "^4.4.2",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "randomstring": "^1.1.5",
    "regenerator-runtime": "^0.11.1",
    "validator": "^9.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/wdjg/coccareerfairapp-server.git"
  }
}
